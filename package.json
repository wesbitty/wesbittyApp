{
  "name": "www",
  "version": "0.0.1",
  "description": "wesbitty official template v1",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "next build",
    "build:app": "prisma generate --schema=./wesbitty/database/schema.prisma && next build",
    "export": "next export",
    "start": "next start",
    "lint": "prettier -w .",
    "dev": "next dev -p 3210",
    "migrate:app": "prisma migrate dev --schema=./wesbitty/database/schema.prisma --skip-seed",
    "reset:app": "prisma migrate reset --schema=./wesbitty/database/schema.prisma --skip-seed",
    "push:app": "prisma db push --schema=./wesbitty/database/schema.prisma",
    "seed:app": "npx prisma db seed",
    "studio:app": "prisma studio --schema=./wesbitty/database/schema.prisma"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} wesbitty/database/seed.ts"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.7",
    "@heroicons/react": "^1.0.6",
