{
  "name": "www",
  "version": "0.0.1",
  "description": "wesbitty official template v1",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "next build",
    "build:all": "npm run generate:app --schema=./wesbitty/database/schema.prisma && next build",
    "export": "next export",
    "start": "next start",
    "lint": "prettier -w .",
    "dev": "next dev -p 3210",
    "generate:app": "prisma generate --schema=./wesbitty/database/schema.prisma",
    "migrate:app": "prisma migrate dev --schema=./wesbitty/database/schema.prisma --skip-seed",
    "reset:app": "prisma migrate reset --schema=./wesbitty/database/schema.prisma --skip-seed",
    "push:app": "prisma db push --schema=./wesbitty/database/schema.prisma",
    "seed:app": "npx prisma db seed",
    "studio:app": "prisma studio --schema=./wesbitty/database/schema.prisma"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.1",
    "@heroicons/react": "^1.0.5",
    "@next-auth/prisma-adapter": "^0.5.4",
    "@prisma/client": "^3.3.0",
    "@tailwindcss/forms": "^0.2.1",
    "bcryptjs": "^2.4.3",
    "classnames": "^2.3.1",
    "lodash": "^4.17.21",
    "next": "^12.0.5",
    "next-auth": "4.19.0",
    "next-connect": "^0.10.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.18.1",
    "react-query": "^3.23.2",
    "superagent": "^6.1.0"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} wesbitty/database/seed.ts"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "^4.0.0-beta.4",
    "@types/bcryptjs": "^2.4.2",
    "@types/lodash": "^4.14.176",
    "@types/node": "^14.18.36",
    "@types/react": "^17.0.3",
    "@types/superagent": "^4.1.13",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "autoprefixer": "^10.2.5",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.0.5",
    "lint-staged": "^11.0.0",
    "mockdate": "^3.0.5",
    "postcss": "^8.2.8",
    "prettier": "^2.3.1",
    "prisma": "^3.3.0",
    "tailwindcss": "^2.2.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.2.3",
    "webpack": "^5.62.1"
  }
}
