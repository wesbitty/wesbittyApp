    "@types/react-copy-to-clipboard": "^4.3.0",
    "@types/react-dom": "^16.9.8",
    "@types/react-syntax-highlighter": "^13.5.0",
    "@types/superagent": "^4.1.13",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
