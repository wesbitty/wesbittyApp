    "globby": "^12.0.2",
    "gray-matter": "^4.0.2",
    "lodash": "^4.17.21",
    "next": "^12.1.5",
    "next-auth": "latest",
    "next-connect": "^0.10.2",
    "next-mdx-remote": "^2.1.3",
    "next-seo": "^4.20.0",
    "react": "^17.0.2",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^17.0.2",
    "react-ga": "^3.3.0",
    "react-hook-form": "^7.18.1",
    "react-markdown": "^5.0.3",
    "react-query": "^3.23.2",
    "react-syntax-highlighter": "^15.3.1",
    "react-tooltip": "^4.2.17",
    "react-transition-group": "^4.4.1",
    "rehype-slug": "^4.0.1",
    "remark": "^13.0.0",
    "remark-gfm": "^1.0.0",
